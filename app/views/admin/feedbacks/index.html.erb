<div id="feedbacks" class="row">
  <div class="col-md-12">
    <div class="post-list white-bg">
      <table class="table table-hover">
        <thead>
          <tr>
            <th class="content"><%= t "feedbacks.subject" %></th>
            <th class="sender"><%= t "feedbacks.sender" %></th>
            <th class="sent_date"><%= t "feedbacks.sent_date" %></th>
          </tr>
        </thead>
        <tbody>
          <%= render @feedbacks %>
        </tbody>
      </table>
      <%= paginate @feedbacks, theme: "twitter-bootstrap-3",
        pagination_class: "pagination-sm pull-right" %>
    </div>
  </div>
</div>
